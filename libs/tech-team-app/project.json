{
  "projectType": "library",
  "root": "libs/tech-team-app",
  "sourceRoot": "libs/tech-team-app/src",
  "prefix": "grid-watch",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/tech-team-app"],
      "options": {
        "jestConfig": "libs/tech-team-app/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/tech-team-app/**/*.ts",
          "libs/tech-team-app/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
